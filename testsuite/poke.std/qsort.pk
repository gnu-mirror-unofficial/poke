/* { dg-do run } */
/* { dg-data {c*} {0x00 0x00 0x00 0x30  0x00 0x00 0x00 0x00  0x00 0x00 0x00 0x20 0x00 0x00 0x00 0x10 } } */

fun cmpints = (any a, any b) int:
  {
   var ai = a as int;
   var bi = b as int;

   if (ai == bi)
     return 0;
   else if (ai < bi)
     return -1;
   else
     return 1;
  }

