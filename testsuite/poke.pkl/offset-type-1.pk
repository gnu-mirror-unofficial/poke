/* This test checks that the units in offset types, when given as
   numerical values, are properly promoted to uint<64>.  */

deftype BPF_Insn_Offset = offset<int<16>,64>;

deftype BPF_Insn =
  struct
  {
    BPF_Insn_Offset offset;
  };

