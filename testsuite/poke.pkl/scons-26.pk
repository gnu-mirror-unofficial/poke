/* { dg-do run } */

var DATUM_UNKNOWN = 0;
var DATUM_NUMBER = 1;
var DATUM_STRING = 2;

type Datum =
  struct {
    int tag;
    union
    {
      int number : tag == DATUM_NUMBER;
      string str : tag == DATUM_STRING;
    } value;
  };

