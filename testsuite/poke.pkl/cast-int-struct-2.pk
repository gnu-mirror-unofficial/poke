deftype Elf64_RelInfo =
  struct uint<32>
  {
    uint<16> r_sym;
    uint<16> r_type;
  };

deftype Elf64_Rel =
  struct
  {
    offset<uint<16>,B> r_offset;
    Elf64_RelInfo r_info;
  };

